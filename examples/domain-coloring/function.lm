export cSin;
export cPow;
export cMul;
export cDiv;

export const t_heta = 0;
export const p_oint = (cos(t_heta), sin(t_heta));

export function r_un(x) {
    cSin(cPow(x, p_oint))
}

export function g_o(a, b, n) {
    cAdd(cMul(a, a), cDiv(b, p_oint))
}

export function i_t(x) {
    state = x;

    define!(ITERATIONS, 1);
    iterate!({
        state = g_o(state, x, get!(ITERATIONS));

        define!(ITERATIONS, ITERATIONS + 1);
    }, ITERATIONS);
}
